<div class="bookings_table">
<nav>

  <nav>
    <ul>
      <li>
          <a href="/gymclasses/bookings/create">Create New Booking</a>
      </li>
    </ul>
  </nav>
  <table>
    <thead>
      <caption><h2>All Class Bookings</h2></caption>
      <tr>
      <th></th>
      <th>Name</th>
      <th>Date</th>
      <th>Time</th>
      <th>Duration</th>
      <th>First Name</th>
      <th>Last Name</th>
    </tr>
    </thead>
    <tbody>
      <% @bookings.each do |booking| %>
        <tr>
          <th><a href="/gymclasses/bookings/<%= booking['id'] %>/get">edit</a></th>
          <td><%= booking['name'] %></td>
          <td><%= booking['start_date']%></td>
          <td><%= booking['start_time']%></td>
          <td><%= booking['duration']%></td>
          <td><%= booking['first_name']%></td>
          <td><%= booking['last_name']%></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
